{
  "compilerOptions": {
    "noImplicitAny": true,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "module": "commonjs",
    "target": "es6",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "lib": [
      "dom",
      "es6",
      "scripthost",
      "es2019.iterable"
    ],
    "noEmitHelpers": true,
    "importHelpers": true,
    "strict": true,
    "outDir": "dist",
    "sourceMap": true
  }
}
